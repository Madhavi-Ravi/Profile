op-hanger-10-10-bag/',\r\n\t\timagecount: 3,\r\n\t\timagelinks:mdsImageArray,\r\n\t},\r\n\tp6:{\r\n\t\tid:6,\r\n\t\ttitle: 'Priceguru(B2B Ecommerce Website)',\r\n\t\ttech: 'Magento2, Javascript, PHP',\r\n\t\tdescription: 'B2C website bulit using Magento2, In which customers can contribute to the events happening for wellness of the people and community.',\r\n\t\tlink: '',\r\n\t\timagecount: 3,\r\n\t\timagelinks:priceguruImageArray,\r\n\t},\r\n}\r\nvar key = \"p\"+props.no;\r\nconsole.log(key, project[key]);\r\n  return (\r\n    <React.Fragment>\r\n      <div class=\"backdrop\" onConfrim={props.onConfrim} />\r\n      \r\n        <ModalOverlay \r\n          title={project[key]['title']}\r\n          tech={project[key]['tech']}\r\n          description={project[key]['description']}\r\n          images={project[key]['imagelinks']}\r\n          link={project[key]['link']}\r\n          imagescount={project[key]['imagescount']}\r\n          onConfirm={props.onConfirm}/>\r\n\r\n        \r\n     \r\n    </React.Fragment>\r\n  )\r\n};\r\n\r\nconst ModalOverlay = (props) => { \r\nconsole.log(props.images);\r\n  return (\r\n    <div class=\"modal\">\r\n        <div class=\"carousel-wrap\" >\r\n            <div class=\"window\">\r\n              <div id=\"carousel\"  class=\"\">\r\n              {props.images.map((ImageArray) => (\r\n\t                    <img class=\"slide\" src={ImageArray} alt=\"responsibility\"/>\r\n              ))}\r\n              <i class=\"mdi mdi-chevron-left\" id=\"prev\"></i>\r\n              <i class=\"mdi mdi-chevron-right\" id=\"next\"></i>\r\n            </div>\r\n          </div>\r\n         </div>\r\n      <div class=\"info-box\">\r\n        <div class=\"title bold\">{props.title}</div>\r\n          <div class=\"tag\">{props.tech}</div>\r\n            <div class=\"detail\">{props.description}</div>\r\n            <a href={props.link} target=\"_blank\"><div class=\"button visible\">\r\n                <i class=\"mdi mdi-open-in-new\"></i>VIEW SITE\r\n          </div></a>\r\n\r\n        <button class=\"close\" id=\"trigger_close\" onClick={props.onConfirm}><VscClose size={30} class=\"close\"/></button>\r\n      </div>      \t\r\n    </div>\r\n  );\r\n};\r\n\r\nconst Backdrop = (props) =>{\r\n  return (<div class=\"backdrop\" onConfrim={props.onClick} />);\r\n};\r\n\r\n\r\nexport default ProjectModel;\r\n",{"ruleId":"73","severity":1,"message":"74","line":1,"column":33,"nodeType":"75","messageId":"76","endLine":1,"endColumn":42},{"ruleId":"73","severity":1,"message":"77","line":3,"column":8,"nodeType":"75","messageId":"76","endLine":3,"endColumn":18},{"ruleId":"78","severity":1,"message":"79","line":15,"column":19,"nodeType":"80","messageId":"81","endLine":15,"endColumn":21},{"ruleId":"78","severity":1,"message":"79","line":26,"column":14,"nodeType":"80","messageId":"81","endLine":26,"endColumn":16},{"ruleId":"78","severity":1,"message":"82","line":26,"column":41,"nodeType":"80","messageId":"81","endLine":26,"endColumn":43},{"ruleId":"83","severity":1,"message":"84","line":11,"column":25,"nodeType":"85","endLine":11,"endColumn":57},{"ruleId":"73","severity":1,"message":"86","line":2,"column":8,"nodeType":"75","messageId":"76","endLine":2,"endColumn":22},{"ruleId":"73","severity":1,"message":"87","line":3,"column":8,"nodeType":"75","messageId":"76","endLine":3,"endColumn":13},{"ruleId":"73","severity":1,"message":"88","line":8,"column":11,"nodeType":"75","messageId":"76","endLine":8,"endColumn":27},{"ruleId":"73","severity":1,"message":"89","line":2,"column":22,"nodeType":"75","messageId":"76","endLine":2,"endColumn":32},{"ruleId":"73","severity":1,"message":"86","line":1,"column":8,"nodeType":"75","messageId":"76","endLine":1,"endColumn":22},{"ruleId":"73","severity":1,"message":"87","line":2,"column":8,"nodeType":"75","messageId":"76","endLine":2,"endColumn":13},{"ruleId":"73","severity":1,"message":"90","line":2,"column":8,"nodeType":"75","messageId":"76","endLine":2,"endColumn":16},{"ruleId":"73","severit